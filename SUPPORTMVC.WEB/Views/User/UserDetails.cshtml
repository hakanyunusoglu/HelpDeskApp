                                {
                                    <input type="text" class="form-control m-input m--font-boldest2" value="Standart Kullanıcı" readonly />
                                }
                                else if (Model.RoleID == 2)
                                {
                                    <input type="text" class="form-control m-input m--font-boldest2" value="Firma Yetkilisi" readonly />
                                }
                                else if (Model.RoleID == 3)
                                {
                                    <input type="text" class="form-control m-input m--font-boldest2" value="HBS Kullanıcı" readonly />
                                }
                                else if (Model.RoleID == 4)
                                {
                                    <input type="text" class="form-control m-input m--font-boldest2" value="HBS Yetkilisi" readonly />
                                }
