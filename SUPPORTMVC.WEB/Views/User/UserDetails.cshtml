                                        string creatoruser;
                                        creatoruser = usr.UName.ToUpper().Substring(0, 1) + usr.UName.Substring(1) + " " + usr.USurname.ToUpper();

                                        @Html.TextBoxFor(model => creatoruser, new { @class = "form-control m-input m--font-boldest2", type = "text", @readonly = "readonly" })
                                    }
