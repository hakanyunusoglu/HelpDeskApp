@using SUPPORTMVC.BLL
@using SUPPORTMVC.ENTITIES.DBT
@model Users
@{
    ViewBag.Title = "UserDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";

    UserManager um = new UserManager();
    RequestManager rm = new RequestManager();
    List<RequestStatus> statuslist = rm.GetReqStatus();
    List<CommentStatus> comstat = rm.GetCommentStatus();
    List<Requests> rlist = rm.GetRequestList();
    List<Users> uslist = um.GetUsers();
    string companyauth = @Model.Company.CompanyAuthName.ToUpper().Substring(0, 1) + @Model.Company.CompanyAuthName.Substring(1) + " " + @Model.Company.CompanyAuthSurname.ToUpper();
}

@if (Session["User"] != null)
{
    int loggeduser = SUPPORTMVC.COMMON.App.Common.GetUserID().Value;

    using (Html.BeginForm())
    {
        @Html.HiddenFor(model => model.UserID)

        <script>
            function sifremigoster() {
                var element = document.getElementById('Password');
                element.type = (element.type == 'password' ? 'text' : 'password');
            }
        </script>

        <div class="m-portlet">
            <div class="m-portlet__head">
                <div class="m-portlet__head-caption">
                    <div class="m-portlet__head-title col-md-4">
