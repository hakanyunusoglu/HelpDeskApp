                            <label class="col-lg-1 col-form-label m--font-bolder">Kul. Adı:</label>
                            <div class="col-md-3">
                                @Html.TextBoxFor(model => model.Username, new { @class = "form-control m-input m--font-boldest2", type = "text", @readonly = "readonly" })
                            </div>
                            <label class="col-lg-1 col-form-label m--font-bolder">Parola:</label>
                            <div class="col-md-3">
                                <div class="input-group">
                                    <input type="password" class="form-control m-input m--font-boldest2" id="Password" name="Password" value="@Model.Password" />
