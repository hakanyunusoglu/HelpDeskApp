                                    <span class="m-badge m-badge--metal m-badge--wide m--font-bolder" style="color: black; font-size: 1.2rem">PASİF</span>
                                }
                            </div>
                        </div>
                        <div class="form-group m-form__group row">
                            <label class="col-lg-1 col-form-label m--font-bolder">Ad Soyad:</label>
                            <div class="col-md-3">
                                @foreach (Users usr in uslist)
                                {
                                    if (usr.UserID == Model.UserID)
                                    {
                                        string usersname;
                                        usersname = usr.UName.ToUpper().Substring(0, 1) + usr.UName.Substring(1) + " " + usr.USurname.ToUpper();

                                        @Html.TextBoxFor(model => usersname, new { @class = "form-control m-input m--font-boldest2", type = "text", @readonly = "readonly" })
                                    }
                                }
                            </div>
                            <label class="col-lg-1 col-form-label m--font-bolder">E-Mail:</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(model => model.Email, new { @class = "form-control m-input m--font-boldest2", type = "text", @readonly = "readonly" })
                            </div>
                        </div>
                        <div class="form-group m-form__group row">
                            <label class="col-lg-1 col-form-label m--font-bolder">Kul. Tipi:</label>
                            <div class="col-md-3">
