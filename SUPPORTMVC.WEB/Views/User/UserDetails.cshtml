                        <span class="m-topbar__userpic" style="margin-left: 0.6rem">
                            <img src="~/Content/assets/img/user_img/@Model.ProfileImage" class="m--marginless" style="width: 170px; height: 170px" alt="" />
                        </span>
                    </div>
                </div>
            </div>
            <div class="col-lg-10">
